' {$STAMP BS2}
' {$PBASIC 2.5}

index VAR Nib

OUTH = %00000000
DIRH = %11111111

index = 0

DO
  FOR index = 0 TO 18
      ' FISH CHIPS And dIP
      LOOKUP index, [%11100001, %01000001, %11100110, %11010101, %00000000,
                     %01100011, %11010101, %01000001, %11110001, %00000000,
                     %11110101, %10000101, %10010111, %00000000,
                     %10010111, %01000001, %11110001], OUTH
      PAUSE 400

  NEXT
LOOP